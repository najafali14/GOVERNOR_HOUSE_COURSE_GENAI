// In Typescript
 let things: string[][] = [['mountain1','mountain2'],['country1','country2'],['city1','city2']]
console.log(things[0])
console.log(things[1])
console.log(things[2])
# In Python
things = [['mountain1','mountain2'],['country1','country2'],['city1','city2']]
print(things[0])
print(things[1])
print(things[2])

