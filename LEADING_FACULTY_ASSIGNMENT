// In Typescript
let alien_color:string = 'green'
if (alien_color=='green'){
   console.log('player earned 5 points')
}
else{
   console.log('failed')
}
# In Python
alien_color = 'green'
if (alien_color=='green'):
 print('player earned 5 points')
else:
 print('failed')


