18
// In Typescript
let places: string[] = ['pakistan','india','iran','turkey','maldeves']
console.log(places) // original array
let s = places.sort() // alphabatical array
console.log(s)
console.log(s.reverse())
let r = places.reverse() // reverse array
console.log(r)
console.log(r.sort())


